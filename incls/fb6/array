//////////////////////////////////
// array namespace

extern single   array.make(args*) = "";
extern single   array.make_dict(args*) = "{ swap array_explode pop }dict";

extern multiple array.first(x) = "array_first";
extern multiple array.last(x) = "array_last";
extern multiple array.next(x,y) = "array_next";
extern multiple array.prev(x,y) = "array_prev";

extern single   array.getitem(x,y) = "array_getitem";
extern single   array.setitem(x,y,z) = "array_setitem";
extern single   array.insertitem(x,y,z) = "array_insertitem";
extern single   array.appenditem(x,y) = "array_appenditem";
extern single   array.delitem(x,y) = "array_delitem";

extern single   array.getrange(x,y) = "array_getrange";
extern single   array.setrange(x,y,z) = "array_setrange";
extern single   array.insertrange(x,y,z) = "array_insertrange";
extern single   array.delrange(x,y,z) = "array_delrange";

extern single   array.union(x,y) = "array_union";
extern single   array.diff(x,y) = "array_diff";
extern single   array.intersect(x,y) = "array_intersect";
extern single   array.nunion(x) = "array_nunion";
extern single   array.ndiff(x) = "array_ndiff";
extern single   array.nintersect(x) = "array_nintersect";

extern single   array.compare(x,y) = "array_compare";
extern single   array.extract(x,y) = "array_extract";
extern single   array.excludeval(x,y) = "array_excludeval";
extern single   array.findval(x,y) = "array_findval";

extern single   array.interpret(x) = "array_interpret";
extern single   array.join(x,y) = "array_join";

extern single   array.keys(x,y) = "array_keys array_make";
extern single   array.vals(x,y) = "array_vals array_make";
extern single   array.matchkey(x,y) = "array_matchkey";
extern single   array.matchval(x,y) = "array_matchval";

extern single   array.nested_get(x,y) = "array_nested_get";
extern single   array.nested_set(x,y,z) = "array_nested_set";
extern single   array.nested_del(x,y) = "array_nested_del";

extern single   array.sort(x,y) = "array_sort";
extern single   array.sort_indexed(x,y,z) = "array_sort_indexed";
extern single   array.reverse(x) = "array_reverse";
extern multiple array.cut(x,y) = "array_cut";


