namespace proc {

extern single mode() = "mode";
extern void   setmode(x) = "setmode";
extern void   preempt() = "preempt";
extern void   foreground() = "foreground";
extern void   background() = "background";
extern single interp(x,y,z) = "interp";
extern single queue(x,y,z) = "queue";
extern single fork() = "fork";
extern single kill(x) = "kill";
extern single pid() = "pid";
extern single ispid?(x) = "ispid?";
extern single getpids(x) = "getpids";
extern single getpidinfo(x) = "getpidinfo";
extern single instances(x) = "instances";

const PREEMPT = muf("pr_mode");
const FOREGROUND = muf("fg_mode");
const BACKGROUND = muf("bg_mode");

}

