extern single   prop.get_val(x,y) = "getpropval";

extern void     prop.add(w,x,y,z) = "addprop";
extern void     prop.set(x,y,z) = "setprop";
extern void     prop.remove(x,y) = "remove_prop";

extern multiple prop.envprop(x,y) = "envprop";
extern multiple prop.envpropstr(x,y) = "envpropstr";

extern single   prop.blessed?(x,y) = "blessed?";
extern void     prop.bless(x,y) = "blessprop";
extern void     prop.unbless(x,y) = "unblessprop";

extern single   prop.dir?(x,y) = "propdir?";
extern single   prop.next(x,y) = "nextprop";

extern single   prop.filter_flags(x,y) = "array_filter_flags";
extern single   prop.filter(x,y,z) = "array_filter_prop";

extern single   prop.dirs_get(x,y) = "array_get_propdirs";

extern single   prop.list_get(x,y) = "array_get_proplist";
extern void     prop.list_put(x,y,z) = "array_put_proplist";

extern single   prop.vals_get(x,y) = "array_get_propvals";
extern void     prop.vals_put(x,y,z) = "array_put_propvals";

extern void     prop.reflist_add(x,y,z) = "reflist_add";
extern void     prop.reflist_del(x,y,z) = "reflist_del";
extern single   prop.reflist_find(x,y,z) = "reflist_find";
extern single   prop.reflist_get(x,y) = "array_get_reflist";
extern void     prop.reflist_put(x,y,z) = "array_put_reflist";

extern single   prop.parse_mpi(w,x,y,z) = "parseprop";
extern multiple prop.parse_mpi_ex(w,x,y,z) = "parsepropex";


