///////////////////////////////
// prog namespace

extern multiple prog.call(x, args*) = "
2 try
    array_explode pop
    depth rotate call
catch abort
endcatch
";

extern multiple prog.call_public(x, pub, args*) = "
3 try
    array_explode pop
    depth rotate depth rotate call
catch abort
endcatch
";

extern multiple prog.execute(addr, args*) = "
2 try
    array_explode 1 + rotate execute
catch abort
endcatch
";

extern single prog.cancall?(x,y) = "cancall?";
extern single prog.compile(x,y) = "compile";
extern void   prog.uncompile(x) = "uncompile";
extern single prog.compiled?(x) = "compiled?";
extern single prog.getlines(x,y,z) = "program_getlines";
extern void   prog.setlines(x,y) = "program_setlines";


