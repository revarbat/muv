///////////////////////////////
// prog namespace

extern multiple prog_call(x, args*) = "
2 try
    array_explode pop
    depth rotate call
catch abort
endcatch
";

extern multiple prog_call_public(x, pub, args*) = "
3 try
    array_explode pop
    depth rotate depth rotate call
catch abort
endcatch
";

extern multiple prog_execute(addr, args*) = "
2 try
    array_explode 1 + rotate execute
catch abort
endcatch
";

extern single prog_cancall?(x,y) = "cancall?";
extern single prog_compile(x,y) = "compile";
extern void   prog_uncompile(x) = "uncompile";
extern single prog_compiled?(x) = "compiled?";
extern single prog_getlines(x,y,z) = "program_getlines";
extern void   prog_setlines(x,y) = "program_setlines";


