var mylvar;

func loopy(count)
{
    var i;
    for (i=1; i<=count; i+=1)
	.tell(intostr(i));

    i = 1;
    do {
	.tell(intostr(i));
	i+=1;
    } while(i<=count);

    i = 1;
    do {
	.tell(intostr(i));
	i+=1;
    } until (i>count);

    i = 1;
    while (i <= count) {
	.tell(intostr(i));
        i += 1;
    }

    var key, val;

    foreach (key => val in online_array()) {
        .tell(array_join(array_make(key, " = ", val, 3)));
    }

    mylvar = 4;
    .tell(intostr(mylvar));
}
